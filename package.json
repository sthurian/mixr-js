{
  "name": "mixr-js",
  "type": "module",
  "version": "0.9.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && chmod 755 build/index.js",
    "build:dev": "tsc && chmod 755 build/index.js",
    "start": "npm run build && node build/index.js",
    "test": "mocha",
    "test:coverage": "c8 npm test",
    "test:coverage:html": "c8 --reporter=html npm test",
    "test:coverage:lcov": "c8 --reporter=lcov npm test",
    "lint": "eslint",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build",
    "release": "npm run build && npm test && npm publish --provenance --access public"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "author": "Steffen Thurian",
  "license": "MIT",
  "description": "A comprehensive TypeScript/JavaScript client library for controlling digital mixing consoles over network. Currently supports Behringer X-Air series (XR12, XR16, XR18) via OSC protocol, with extensible architecture designed for future mixer support across different protocols.",
  "keywords": [
    "behringer",
    "x-air",
    "xr12",
    "xr16",
    "xr18",
    "mixer",
    "audio",
    "osc",
    "music",
    "sound",
    "digital-mixer",
    "mixing-console",
    "typescript",
    "node",
    "mixr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sthurian/mixr-js.git"
  },
  "bugs": {
    "url": "https://github.com/sthurian/mixr-js/issues"
  },
  "homepage": "https://github.com/sthurian/mixr-js#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.21",
    "@types/node-osc": "^9.1.0",
    "@types/sinon": "^17.0.4",
    "c8": "^10.1.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "fishery": "^2.3.1",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "mocha": "^11.5.0",
    "prettier": "^3.6.2",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "osc-min": "^2.1.2",
    "zod": "^4.1.1"
  }
}
